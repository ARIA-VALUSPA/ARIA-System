<?xml version="1.0" encoding="UTF-8"?>
<taskModel xmlns="http://www.cs.wpi.edu/~rich/cetask/cea-2018-ext"
           xmlns:disco="urn:disco.wpi.edu:Disco"
           about="urn:fr.telecom-paristech:ARIA-VALUSPA:demo">
   <task id="Welcome">
      <subtasks id="_Welcome_subtasks">
         <step name="_Welcome_step" task="disco:Say$Expression"/>
         <step name="_Welcome_ref" task="_Welcome_tree"/>
         <binding slot="$_Welcome_step.text" value="&#34;(inputPath)+\&#34;Welcome.xml\&#34;&#34;"/>
         <binding slot="$_Welcome_step.external" value="false"/>
      </subtasks>
   </task>
   <task id="Presentation">
      <subtasks id="_Presentation_subtasks">
         <step name="_Presentation_step" task="disco:Say$Expression"/>
         <step name="_Presentation_ref" task="_Presentation_tree"/>
         <binding slot="$_Presentation_step.text" value="&#34;(inputPath)+\&#34;Presentation.xml\&#34;&#34;"/>
         <binding slot="$_Presentation_step.external" value="false"/>
      </subtasks>
   </task>
   <task id="GoodBye">
      <subtasks id="_GoodBye_subtasks">
         <step name="_GoodBye_step" task="disco:Say$Expression"/>
         <step name="_GoodBye_ref" task="_GoodBye_tree"/>
         <binding slot="$_GoodBye_step.text" value="&#34;(inputPath)+\&#34;GoodBye.xml\&#34;&#34;"/>
         <binding slot="$_GoodBye_step.external" value="false"/>
      </subtasks>
   </task>
   <task id="_Welcome_tree">
      <subtasks id="_d4e13_subtasks">
         <step name="_d4e13_step" task="disco:Say"/>
         <binding slot="$_d4e13_step.text" value="&#34;Nice&#34;"/>
         <binding slot="$_d4e13_step.external" value="true"/>
      </subtasks>
   </task>
   <task id="_Presentation_tree">
      <subtasks id="_d4e15_subtasks">
         <step name="_d4e15_step" task="disco:Say"/>
         <binding slot="$_d4e15_step.text" value="&#34;I see&#34;"/>
         <binding slot="$_d4e15_step.external" value="true"/>
      </subtasks>
   </task>
   <task id="_GoodBye_tree">
      <subtasks id="_d4e17_subtasks">
         <step name="_d4e17_step" task="disco:Say"/>
         <binding slot="$_d4e17_step.text" value="&#34;Thanks&#34;"/>
         <binding slot="$_d4e17_step.external" value="true"/>
      </subtasks>
   </task>
   <task id="Top">
      <subtasks id="subTop">
         <step name="welcome" task="Welcome"/>
         <step name="presentation" task="Presentation"/>
         <step name="goodbye" task="GoodBye"/>
      </subtasks>
   </task>
   <script init="true"> 

	var inputPath = "/DiscoScenarios/FML/Demo/"

	
</script>
</taskModel>