# visualization

show:plot = true
show:gui = true
show:windows = true

# camera

camera:fps = 25

# logging

log:use  	= true
log:dir 	= log

# active mq

activemq:use = true
activemq:uri = failover:(tcp://localhost:61616)		# activemq uri
activemq:id = SSI									# activemq id
activemq:topic= SSI									# activemq topic