<?xml version="1.0" ?>

<ssi>
	<face id="$(stream=0;select=0)">				
		<emotion>
			<neutral>$(event=emotion@emax;select=0)</neutral>		
			<anger>$(event=emotion@emax;select=1)</anger>		
			<disgust>$(event=emotion@emax;select=2)</disgust>
			<fear>$(event=emotion@emax;select=3)</fear>
			<happiness>$(event=emotion@emax;select=4)</happiness>
			<sadness>$(event=emotion@emax;select=5)</sadness>		
			<surprised>$(event=emotion@emax;select=6)</surprised>		
		</emotion>
	</face>		
</ssi>

